#include<stdio.h>
void main() {
	
int i,n,key,a[100],flag=0,pos;
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	scanf("%d",&key);
	for(i=0;i<n;i++){
		if(a[i]==key){
			pos=i;
			flag=1;
			break;
		}
	}
	
	if (flag == 1) {
		printf("found at position %d\n", pos);
	} else {
		printf("%d not found\n", key);
	}
}

